{
  "compilerOptions": {
    "rootDirs": ["./src", "./test/cpro/js/ui5/mobx/test-infra"],
    "outDir": "./lib",
    "paths": {
      "cpro/js/ui5/mobx/*": ["./src/cpro/js/ui5/mobx/*"]
    },
    "target": "ES2016",
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": ["../../node_modules/@types", "../../node_modules/@openui5/ts-types-esm"],
    "strict": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "include": ["./src", "test", ".types"],
  "exclude": ["node_modules", "lib"]
}
