{
  "name": "ui5-mobx",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Integration of mobx into UI5",
  "license": "MIT",
  "repository": "git@github.com:texttechne/ui5-mobx.git",
  "author": "texttechne",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "check-circular-deps": "madge ./src --extensions ts --circular",
    "clean": "rimraf lib build",
    "compile": "tsc",
    "prepare": "husky install",
    "prepublish": "yarn build",
    "test": "jest --passWithNoTests ./test "
  },
  "files": [
    "*.md",
    "bin",
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "mobx",
    "ui5",
    "typescript",
    "ts"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "env-cmd": "^10.1.0",
    "husky": "^8.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "madge": "^6.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-properties": "^0.1.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "types": "./lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "readme": "README.md"
}
