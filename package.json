{
  "name": "ui5-mobx",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Integration of mobx into UI5",
  "license": "MIT",
  "repository": "git@github.com:texttechne/ui5-mobx.git",
  "author": "texttechne",
  "main": "lib/index.js",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "check-circular-deps": "madge ./src --extensions ts --circular",
    "compile": "tsc",
    "prepare": "husky install",
    "prepublish": "yarn build",
    "test": "karma start --singleRun",
    "testing": "karma start"
  },
  "files": [
    "*.md",
    "bin",
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "mobx",
    "ui5",
    "typescript",
    "ts"
  ],
  "dependencies": {
    "mobx": "^6.8.0"
  },
  "devDependencies": {
    "@openui5/ts-types-esm": "1.111.0",
    "@types/jasmine": "^4.3.1",
    "@types/jest": "^29.4.0",
    "@types/karma-jasmine": "^4.0.2",
    "@types/node": "^18.14.1",
    "@ui5/cli": "^3.0.2",
    "env-cmd": "^10.1.0",
    "import-sort-style-module": "^6.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-ui5": "^2.4.0",
    "madge": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-properties": "^0.1.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "ui5-middleware-livereload": "^0.6.0",
    "ui5-tooling-modules": "^0.7.10",
    "ui5-tooling-transpile": "^0.2.0"
  },
  "types": "./lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "readme": "README.md",
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-tooling-transpile",
      "ui5-tooling-modules"
    ]
  }
}
