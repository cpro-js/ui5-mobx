{
  "compilerOptions": {
    "rootDirs": ["./src"],
    "outDir": "./dist/resources",
    "paths": {
      "cpro/js/ui5/mobx/*": ["./src/cpro/js/ui5/mobx/*"]
    },
    "target": "ES2016",
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    //    "types": ["node"],
    "typeRoots": ["./node_modules/@types", "./node_modules/@openui5/ts-types-esm"],
    "strict": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "include": ["./src", ".types"],
  "exclude": ["node_modules", "lib"]
}
